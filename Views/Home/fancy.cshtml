


<!doctype html>
<head>
    <title>Fancy</title>
    <script type="text/javascript">
        $(document).ready(function () {
            $('.fancybox').fancybox();

            /*
             *  Different effects
             */

            // Change title type, overlay closing speed
            $(".fancybox-effects-a").fancybox({
                helpers: {
                    title: {
                        type: 'outside'
                    },
                    overlay: {
                        speedOut: 0
                    }
                }
            });

        });

    </script>


    <!-- Add mousewheel plugin (this is optional) -->
    <script type="text/javascript" src="~/Content/Images/helpers/jquery.mousewheel-3.0.6.pack.js"></script>

    <!-- Add fancyBox -->
    <link rel="stylesheet" href="~/Content/Images/jquery.fancybox.css?v=2.1.5" type="text/css" media="screen" />
    <script type="text/javascript" src="~/Content/Images/jquery.fancybox.pack.js?v=2.1.5"></script>

    <!-- Optionally add helpers - button, thumbnail and/or media -->
    <link rel="stylesheet" href="~/Content/Images/helpers/jquery.fancybox-buttons.css?v=1.0.5" type="text/css" media="screen" />
    <script type="text/javascript" src="~/Content/Images/jquery.fancybox-buttons.js?v=1.0.5"></script>
    <script type="text/javascript" src="~/Content/Images/helpers/jquery.fancybox-media.js?v=1.0.6"></script>

    <link rel="stylesheet" href="~/Content/Images/helpers/jquery.fancybox-thumbs.css?v=1.0.7" type="text/css" media="screen" />
    <script type="text/javascript" src="~/Content/Images/helpers/jquery.fancybox-thumbs.js?v=1.0.7"></script>

</head>
