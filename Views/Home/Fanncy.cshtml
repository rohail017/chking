<!doctype html>
<head>
    <title>Fancy</title>
    
    @* $(document).ready(function () {
            $(".fancybox").fancybox();
        });*@

    <!-- Add jQuery library -->
    <!-- Add jQuery library -->
    <script type="text/javascript" src="~/Content/helpers/jquery-1.10.1.min.js"></script>
    <!-- Add mousewheel plugin (this is optional) -->
    <script type="text/javascript" src="~/Content//helpers/jquery.mousewheel-3.0.6.pack.js"></script>
    <!-- Add fancyBox -->
    <link rel="stylesheet" href="~/Content/jquery.fancybox.css?v=2.1.5" type="text/css" media="screen" />
    <script type="text/javascript" src="~/Content/jquery.fancybox.pack.js?v=2.1.5"></script>
    <!-- Add Button helper (this is optional) -->
    <link rel="stylesheet" type="text/css" href="~/Content/helpers/jquery.fancybox-buttons.css?v=1.0.5" />
    <script type="text/javascript" src="~/Content/helpers/jquery.fancybox-buttons.js?v=1.0.5"></script>
    <!-- Add Thumbnail helper (this is optional) -->
    <link rel="stylesheet" type="text/css" href="~/Content/helpers/jquery.fancybox-thumbs.css?v=1.0.7" />
    <script type="text/javascript" src="~/Content/helpers/jquery.fancybox-thumbs.js?v=1.0.7"></script>
    <!-- Add Media helper (this is optional) -->
    <script type="text/javascript" src="~/Content/helpers/jquery.fancybox-media.js?v=1.0.6"></script>

    <script>
       
        $(document).ready(function () {
            $('.fancybox').fancybox();
        });
    </script>
    <style type="text/css">
        .fancybox-custom.fancybox-skin {
            box-shadow: 0 0 50px #222;
        }

        body {
            max-width: 900px;
            margin: 0 auto;
            background-repeat:no-repeat;
            background-size:cover;
        }
    </style>
  
   


</head>
